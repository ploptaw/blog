{
  "name": "blog",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "cross-env npx @11ty/eleventy",
    "dev": "cross-env npx @11ty/eleventy --serve",
    "compress": "npx lightningcss-cli --minify --bundle --targets \">= 0.25%, not dead\" src/style.css -o src/style.min.css && npx uglifyjs ./src/js/lottery.js -c -m --output ./src/js/lottery.min.js && npx uglifyjs ./src/js/unicode-convert.js -c -m --output ./src/js/unicode-convert.min.js",
    "watch": "chokidar src/style.css sec/js/*.js -c \"npm run compress\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ploptaw/blog.git"
  },
